require 'web;

web_server 1234
{<in out>
    print "Got a client";
    print in() in();

    out "HTTP/1.0 200 OK\r\n";
    out "Content-type: text/html\r\n";
    out "\r\n";

    i = 0;
    repeat
    {
        out "Jellyfish " + i.to_string + "<br>\r\n";
        i = i + 1;
        i < 10;
    };

    out \n;
};
