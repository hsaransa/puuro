# Define other comparisons by using < and ==

t = 123.type;
t.set_method 'ne {<a b> !(a == b)};
t.set_method 'gt {<a b> !(a <= b)};
t.set_method 'le {<a b> (a < b).or a == b};
t.set_method 'ge {<a b> (a > b).or a == b};

std.print 1 != 2;
std.print 1 > 2;
std.print 1 <= 2;
std.print 1 >= 2;


