# List extensions
# TODO: any, all, foldr, foldl = fold, zip, unzip

t = [].type;

t.set_method 'length {<l> l.size };
t.set_method 'len {<l> l.size };

t.set_method 'filter {<l, proc>
    ret = [];
    i = 0;

    std.repeat
    {
        (proc (l.at i)).then {
            ret.append (l.at i)
        };

        i = i + 1;

        i < l.size;
    };

    ret;
};
